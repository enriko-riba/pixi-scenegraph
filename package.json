{
  "name": "pixi-scenegraph",
  "version": "1.8.10",
  "description": "typescript scene engine for pixi.js",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "bump": "node version.js",
    "doc": "./node_modules/.bin/typedoc --out docs/generated --mode file",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint",
    "prePublish": "npm run bump && npm run format && npm run build && npm run doc",
    "build": "npm run clean && parcel build src/index.ts  ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "pixi",
    "scenegraph",
    "game-engine",
    "game-dev"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enriko-riba/pixi-scenegraph.git"
  },
  "author": "Enriko Riba",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "parcel-bundler": "~1.12.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "pixi.js": "^5.3.3"
  }
}
