{
  "name": "pixi-scenegraph",
  "version": "1.8.12",
  "description": "typescript scene engine for pixi.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "bump": "node version.js",
    "doc": "./node_modules/.bin/typedoc --out docs/generated --mode file",
    "format": "prettier --write \"./**/*.ts\"",
    "lint": "eslint",
    "prePublish": "npm run bump && npm run format && npm run build && npm run doc",
    "build": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "pixi",
    "scenegraph",
    "game-engine",
    "game-dev"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enriko-riba/pixi-scenegraph.git"
  },
  "author": "Enriko Riba",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "prettier": "2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.0.0",
    "webpack-node-externals": "^2.5.0"
  },
  "dependencies": {
    "pixi.js": "^5.3.3"
  }
}
